{
  "name": "semi-admin",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "biome check --write",
    "clean": "rimraf **/node_modules && rimraf node_modules",
    "prepare": "lefthook install",
    "commit": "git status && git add -A && git-cz && git push",
    "reset": "npx rimraf node_modules ./**/node_modules",
    "dev": "turbo run dev --parallel",
    "dev:build": "turbo run dev-build --parallel",
    "dev:web": "turbo run dev --filter=admin-web",
    "dev:server": "turbo run dev --filter=admin-server",
    "dev:admin": "pnpm -C apps/admin-web dev",
    "dev:admin-server": "pnpm -C server/admin-server dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.12.4",
  "dependencies": {
    "@ahooksjs/use-url-state": "catalog:app",
    "@douyinfe/semi-ui": "catalog:semi-ui",
    "@modern-js/runtime": "catalog:build",
    "ahooks": "catalog:app",
    "axios": "catalog:app",
    "i18next": "catalog:react",
    "i18next-browser-languagedetector": "catalog:react",
    "lodash-es": "catalog:app",
    "npm-run-all": "^4.1.5",
    "qs": "catalog:app",
    "react": "catalog:react",
    "react-dom": "catalog:react",
    "react-i18next": "catalog:react",
    "zustand": "catalog:react"
  },
  "devDependencies": {
    "@biomejs/biome": "catalog:lint",
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@modern-js/app-tools": "catalog:build",
    "@modern-js/tsconfig": "catalog:build",
    "@tailwindcss/postcss": "catalog:tailwindcss",
    "@types/jest": "catalog:react",
    "@types/lodash-es": "catalog:app",
    "@types/node": "catalog:node",
    "@types/qs": "catalog:app",
    "@types/react": "catalog:react",
    "@types/react-dom": "catalog:react",
    "clsx": "catalog:tailwindcss",
    "commitizen": "^4.3.0",
    "cz-git": "^1.7.0",
    "lefthook": "^2.0.4",
    "postcss": "catalog:tailwindcss",
    "postcss-loader": "catalog:tailwindcss",
    "rimraf": "^6.0.1",
    "tailwind-merge": "catalog:tailwindcss",
    "tailwindcss": "catalog:tailwindcss",
    "turbo": "^2.4.0",
    "typescript": "catalog:node"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "engines": {
    "node": ">=16.18.1"
  },
  "pnpm": {
    "overrides": {
      "react": "18.3.1",
      "react-dom": "18.3.1",
      "@types/react": "18.3.11",
      "@types/react-dom": "18.3.1"
    }
  }
}
